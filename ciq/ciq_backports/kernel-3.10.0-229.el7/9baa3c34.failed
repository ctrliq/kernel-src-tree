+ static u64 snb_uncore_imc_read_counter(struct intel_uncore_box *box, struct perf_event *event)
+ {
+ 	struct hw_perf_event *hwc = &event->hw;
+ 
+ 	return (u64)*(unsigned int *)(box->io_addr + hwc->event_base);
+ }
