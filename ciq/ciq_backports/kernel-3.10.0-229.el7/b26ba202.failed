 	struct proto *answer_prot;
 	unsigned char answer_flags;
-	char answer_no_check;
 	int try_loading_module = 0;
 	int err;
 
@@ -166,7 +165,6 @@ lookup_protocol:
 
 	sock->ops = answer->ops;
