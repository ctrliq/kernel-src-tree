+ static inline void __user *kvm_get_attr_addr(struct kvm_device_attr *attr)
+ {
+ 	void __user *uaddr = (void __user*)(unsigned long)attr->addr;
+ 
+ 	if ((u64)(unsigned long)uaddr != attr->addr)
+ 		return ERR_PTR_USR(-EFAULT);
+ 	return uaddr;
