+ static struct mlx5e_async_ctx *mlx5e_bulk_async_init(struct mlx5_core_dev *mdev, int n)
+ {
+ 	struct mlx5e_async_ctx *bulk_async;
+ 	int i;
+ 
+ 	bulk_async = kvcalloc(n, sizeof(struct mlx5e_async_ctx), GFP_KERNEL);
+ 	if (!bulk_async)
