+ static void mlx5e_tls_priv_tx_cleanup(struct mlx5e_ktls_offload_context_tx *priv_tx,
+ 				      struct mlx5e_async_ctx *async)
+ {
+ 	if (priv_tx->create_err) {
+ 		complete(&async->complete);
+ 		kfree(priv_tx);
+ 		return;
