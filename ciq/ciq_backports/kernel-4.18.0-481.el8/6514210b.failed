+ {
+ 	struct mlx5e_ktls_offload_context_tx *priv_tx;
+ 	int err;
+ 
+ 	priv_tx = kzalloc(sizeof(*priv_tx), GFP_KERNEL);
+ 	if (!priv_tx)
+ 		return ERR_PTR(-ENOMEM);
