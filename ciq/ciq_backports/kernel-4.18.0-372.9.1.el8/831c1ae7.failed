diff --git a/arch/x86/include/asm/hyperv-tlfs.h b/arch/x86/include/asm/hyperv-tlfs.h
index 07c046c2e3b8..9eb549543030 100644
--- a/arch/x86/include/asm/hyperv-tlfs.h
