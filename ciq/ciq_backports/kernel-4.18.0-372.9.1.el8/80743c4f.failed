+ 	err = mlx5e_safe_switch_params(priv, &new_params, preactivate, NULL, true);
+ 	if (err && rl) {
+ 		mlx5e_mqprio_rl_cleanup(rl);
+ 		mlx5e_mqprio_rl_free(rl);
+ 	}
+ 
+ 	return err;
+ }
