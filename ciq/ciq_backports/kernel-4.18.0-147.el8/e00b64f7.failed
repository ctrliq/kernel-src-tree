+ 		if (!wq->sw_rq)
+ 			goto err_put_qpid;
+ 		wq->pending_wrs = kcalloc(srq->wq.size,
+ 					  sizeof(*srq->wq.pending_wrs),
+ 					  GFP_KERNEL);
