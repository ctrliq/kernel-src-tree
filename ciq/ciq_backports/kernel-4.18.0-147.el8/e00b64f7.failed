+ 	dma_addr_t phy_prod_pair_addr;
+ 	u32 num_elems;
+ 	void *va;
+ 	int rc;
+ 
+ 	va = dma_alloc_coherent(&dev->pdev->dev,
+ 				sizeof(struct rdma_srq_producers),
+ 				&phy_prod_pair_addr, GFP_KERNEL);
