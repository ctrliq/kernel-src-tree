+ void c4iw_copy_wr_to_srq(struct t4_srq *srq, union t4_recv_wr *wqe, u8 len16)
+ {
+ 	u64 *src, *dst;
+ 
+ 	src = (u64 *)wqe;
+ 	dst = (u64 *)((u8 *)srq->queue + srq->wq_pidx * T4_EQ_ENTRY_SIZE);
+ 	while (len16) {
+ 		*dst++ = *src++;
+ 		if (dst >= (u64 *)&srq->queue[srq->size])
+ 			dst = (u64 *)srq->queue;
+ 		*dst++ = *src++;
+ 		if (dst >= (u64 *)&srq->queue[srq->size])
+ 			dst = (u64 *)srq->queue;
+ 		len16--;
+ 	}
+ }
