 + */
 +static int netiucv_pm_restore_thaw(struct device *dev)
 +{
 +	struct netiucv_priv *priv = dev_get_drvdata(dev);
 +	struct net_device *ndev = NULL;
 +	int rc = 0;
 +
 +	IUCV_DBF_TEXT(trace, 3, __func__);
 +	if (priv && priv->conn)
 +		ndev = priv->conn->netdev;
 +	if (!ndev)
