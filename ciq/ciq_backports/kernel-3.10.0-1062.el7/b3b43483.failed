+ static int qedr_idr_add(struct qedr_dev *dev, struct qedr_idr *qidr,
+ 			void *ptr, u32 id);
+ static void qedr_idr_remove(struct qedr_dev *dev,
+ 			    struct qedr_idr *qidr, u32 id);
