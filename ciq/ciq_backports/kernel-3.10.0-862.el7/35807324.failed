+ static int devlink_dpipe_send_and_alloc_skb(struct sk_buff **pskb,
+ 					    struct genl_info *info)
+ {
+ 	int err;
+ 
+ 	if (*pskb) {
+ 		err = genlmsg_reply(*pskb, info);
+ 		if (err)
+ 			return err;
+ 	}
+ 	*pskb = genlmsg_new(GENLMSG_DEFAULT_SIZE, GFP_KERNEL);
+ 	if (!*pskb)
+ 		return -ENOMEM;
+ 	return 0;
+ }
